{
  "name": "bankarstvo",
  "version": "1.0.0",
  "description": "Banking application with modern architecture",
  "main": "index.js",
  "scripts": {
    "test": "pytest && npm run lint",
    "build": "tsc && webpack --config webpack.config.js",
    "dev": "webpack serve --mode development",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "test:unit": "pytest",
    "test:lint": "eslint . --ext .ts,.tsx",
    "test:all": "npm run test:unit && npm run test:lint",
    "css:build": "tailwindcss -i ./static/css/input.css -o ./static/css/main.css",
    "css:watch": "tailwindcss -i ./static/css/input.css -o ./static/css/main.css --watch"
  },
  "author": "koke1997",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "vue": "^3.2.26",
    "chart.js": "^3.6.0",
    "axios": "^1.3.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.0",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.35.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
