    "test:all": "npm run test:unit && npm run test:lint",
    "css:build": "tailwindcss -i ./static/css/input.css -o ./static/css/main.css",
    "css:watch": "tailwindcss -i ./static/css/input.css -o ./static/css/main.css --watch"
  },
